{
  "name": "tokyo-nextjs",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "seed:cv": "tsx scripts/seed-cv.ts",
    "db:seed": "tsx seed/index.ts",
    "db:seed:force": "tsx seed/index.ts --force",
    "db:seed:albums": "tsx seed/index.ts --only=albums",
    "db:seed:videos": "tsx seed/index.ts --only=videos",
    "db:seed:services": "tsx seed/index.ts --only=services",
    "db:seed:resume": "tsx seed/index.ts --only=resume_entries",
    "db:seed:settings": "tsx seed/index.ts --only=site_settings",
    "db:seed:users": "tsx seed/index.ts --only=users",
    "db:backup": "tsx seed/backup.ts",
    "db:backup:overwrite": "tsx seed/backup.ts --overwrite",
    "db:clean": "tsx seed/clean.ts",
    "db:clean:yes": "tsx seed/clean.ts --yes",
    "db:reset": "tsx seed/backup.ts && prisma migrate reset --force",
    "db:reset:seed": "tsx seed/backup.ts && prisma migrate reset --force && tsx seed/index.ts",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed:prod": "tsx scripts/db/seed-prod.ts",
    "db:migrate:prod": "tsx scripts/db/migrate-prod.ts",
    "db:status:prod": "tsx scripts/db/status-prod.ts",
    "vercel-build": "node scripts/vercel-build.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ],
    "prisma/schema.prisma": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-pdf/renderer": "^4.3.1",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.0",
    "@tiptap/extension-link": "^3.11.0",
    "@tiptap/extension-placeholder": "^3.11.0",
    "@tiptap/react": "^3.11.0",
    "@tiptap/starter-kit": "^3.11.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "eslint": "9",
    "eslint-config-next": "^16.0.3",
    "framer-motion": "^12.23.24",
    "isomorphic-dompurify": "^2.33.0",
    "lucide-react": "^0.554.0",
    "maath": "^0.10.8",
    "mapbox-gl": "^3.16.0",
    "next": "^16.0.7",
    "next-intl": "^4.5.5",
    "next-themes": "^0.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.66.1",
    "react-map-gl": "^8.1.0",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^3.4.0",
    "three": "^0.181.2",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "24.10.1",
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "@types/three": "^0.181.0",
    "autoprefixer": "^10.4.22",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^6.19.0",
    "tailwindcss": "3",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0"
  }
}
